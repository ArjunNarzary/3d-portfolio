{
    "plugins": ["@trivago/prettier-plugin-sort-import"],
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "importOrder": ["^react", "^@", "^react$", "^components/(.*)$", "^[./]", "^(?!.*.s?css$)^[./^[a-zA-Z_]+$", "\\.s?css$" ],
    "importOrderSeparation": true,
    "importOrderSortSpecifier": true
}